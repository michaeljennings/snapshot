{
  "name": "michaeljennings/snapshot",
  "description": "A PHP package that takes a snapshot of your application and allows you to retrieve it later to help with debugging.",
  "keywords": [
    "debugging",
    "snapshot",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Jennings",
      "email": "michael.jennings91@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "maknz/slack": "1.7.*",
    "league/event": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Michaeljennings\\Snapshot\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^3.5"
  },
  "autoload-dev": {
      "psr-4": {
          "Michaeljennings\\Snapshot\\Tests\\": "tests/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Michaeljennings\\Snapshot\\SnapshotServiceProvider"
          ]
      }
  }
}
